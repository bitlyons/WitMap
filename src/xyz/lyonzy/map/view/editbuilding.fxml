<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>
<TabPane xmlns="http://javafx.com/javafx/null" xmlns:fx="http://javafx.com/fxml/1"
         fx:controller="xyz.lyonzy.map.controller.EditBuildingController">
    <tabs>
        <Tab text="Building Info">
            <content>
                <FlowPane alignment="CENTER" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity"
                          minWidth="-Infinity" prefHeight="400.0" prefWidth="600.0">
                    <children>
                        <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Building Info">
                            <font>
                                <Font name="Arial Black" size="23.0"/>
                            </font>
                        </Text>
                        <GridPane gridLinesVisible="true" prefHeight="327.0" prefWidth="577.0">
                            <columnConstraints>
                                <ColumnConstraints hgrow="SOMETIMES" maxWidth="284.0" minWidth="10.0"
                                                   prefWidth="115.0"/>
                                <ColumnConstraints hgrow="SOMETIMES" maxWidth="468.0" minWidth="10.0"
                                                   prefWidth="462.0"/>
                            </columnConstraints>
                            <rowConstraints>
                                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                                <RowConstraints maxHeight="51.0" minHeight="10.0" prefHeight="51.0" vgrow="SOMETIMES"/>
                                <RowConstraints maxHeight="126.0" minHeight="10.0" prefHeight="110.0"
                                                vgrow="SOMETIMES"/>
                                <RowConstraints maxHeight="109.0" minHeight="10.0" prefHeight="99.0" vgrow="SOMETIMES"/>
                                <RowConstraints maxHeight="49.0" minHeight="10.0" prefHeight="23.0" vgrow="SOMETIMES"/>
                            </rowConstraints>
                            <children>
                                <Label text="Building No : ">
                                    <font>
                                        <Font name="Arial Black" size="12.0"/>
                                    </font>
                                    <GridPane.margin>
                                        <Insets/>
                                    </GridPane.margin>
                                </Label>
                                <Label text="Building Name : " GridPane.rowIndex="1">
                                    <font>
                                        <Font name="Arial Black" size="12.0"/>
                                    </font>
                                </Label>
                                <Label text="Building Info" GridPane.rowIndex="2">
                                    <font>
                                        <Font name="Arial Black" size="12.0"/>
                                    </font>
                                </Label>
                                <Label text="Opening Hours" GridPane.rowIndex="3">
                                    <font>
                                        <Font name="Arial Black" size="12.0"/>
                                    </font>
                                </Label>
                                <Label text="Image Url" GridPane.rowIndex="4">
                                    <font>
                                        <Font name="Arial Black" size="12.0"/>
                                    </font>
                                </Label>
                                <TextField fx:id="bId" disable="true" GridPane.columnIndex="1"/>
                                <TextField fx:id="bName" GridPane.columnIndex="1" GridPane.rowIndex="1"/>
                                <TextArea fx:id="bInfo" prefHeight="200.0" prefWidth="200.0" GridPane.columnIndex="1"
                                          GridPane.rowIndex="2"/>
                                <TextArea fx:id="bOpeningHours" prefHeight="200.0" prefWidth="200.0"
                                          GridPane.columnIndex="1" GridPane.rowIndex="3"/>
                                <TextField fx:id="biURL" GridPane.columnIndex="1" GridPane.rowIndex="4"/>
                            </children>
                            <opaqueInsets>
                                <Insets/>
                            </opaqueInsets>
                            <FlowPane.margin>
                                <Insets bottom="5.0" left="5.0" right="5.0" top="5.0"/>
                            </FlowPane.margin>
                        </GridPane>
                        <Button fx:id="submit" mnemonicParsing="false" onAction="#submit" text="Submit">
                            <FlowPane.margin>
                                <Insets bottom="5.0" right="5.0" top="5.0"/>
                            </FlowPane.margin>
                            <font>
                                <Font name="Arial" size="12.0"/>
                            </font>
                        </Button>
                        <Button fx:id="cancel" mnemonicParsing="false" onAction="#close" text="Cancel">
                            <FlowPane.margin>
                                <Insets bottom="5.0" left="5.0" right="5.0" top="5.0"/>
                            </FlowPane.margin>
                            <font>
                                <Font name="Arial" size="12.0"/>
                            </font>
                        </Button>
                    </children>
                </FlowPane>
            </content>
        </Tab>
        <Tab text="Rooms">
            <content>
                <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0"/>
            </content>
        </Tab>
    </tabs>
</TabPane>
